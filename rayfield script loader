-- Load Rayfield Library
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Create Window
local Window = Rayfield:CreateWindow({
   Name = "Rayfield Script Hub",
   LoadingTitle = "Rayfield Interface Suite",
   LoadingSubtitle = "by Sirius",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "RayfieldHub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

-- Create Tabs
local ScriptsTab = Window:CreateTab("üìú Scripts", 4483362458)
local PlayerTab = Window:CreateTab("üë§ Player", 4483362458)
local MiscTab = Window:CreateTab("‚öôÔ∏è Misc", 4483362458)

-- Scripts Tab
local ScriptsSection = ScriptsTab:CreateSection("Available Scripts")

Rayfield:Notify({
   Title = "Script Hub Loaded",
   Content = "Rayfield Hub has been loaded successfully!",
   Duration = 6.5,
   Image = 4483362458,
})

-- TAT Script Button
local TATButton = ScriptsTab:CreateButton({
   Name = "Execute TAT ESP Script",
   Callback = function()
      Rayfield:Notify({
         Title = "Loading Script",
         Content = "Loading TAT ESP & Teleport...",
         Duration = 3,
         Image = 4483362458,
      })
      
      local success, err = pcall(function()
         loadstring(game:HttpGet('https://raw.githubusercontent.com/forrpus3/scripts/refs/heads/main/V2%20TAT'))()
      end)
      
      if success then
         Rayfield:Notify({
            Title = "Success!",
            Content = "TAT Script executed successfully!",
            Duration = 5,
            Image = 4483362458,
         })
      else
         Rayfield:Notify({
            Title = "Error",
            Content = "Failed to load TAT Script: " .. tostring(err),
            Duration = 5,
            Image = 4483362458,
         })
      end
   end,
})

local TATLabel = ScriptsTab:CreateLabel("TAT ESP - Paper & Meteor ESP with Teleportation")

local ScriptsParagraph = ScriptsTab:CreateParagraph({
   Title = "About TAT Script",
   Content = "This script provides ESP highlighting for Papers (Green) and Meteors (Red), along with teleportation features and auto-farming capabilities."
})

-- Add more script buttons
local ScriptsSection2 = ScriptsTab:CreateSection("More Scripts")

