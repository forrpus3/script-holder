-- Player Tab
local PlayerSection = PlayerTab:CreateSection("Player Modifications")

local WalkSpeedSlider = PlayerTab:CreateSlider({
   Name = "Walk Speed",
   Range = {16, 100},
   Increment = 1,
   Suffix = " Speed",
   CurrentValue = 16,
   Flag = "WalkSpeed",
   Callback = function(Value)
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
   end,
})

local JumpPowerSlider = PlayerTab:CreateSlider({
   Name = "Jump Power",
   Range = {50, 200},
   Increment = 1,
   Suffix = " Power",
   CurrentValue = 50,
   Flag = "JumpPower",
   Callback = function(Value)
      game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
   end,
})

local InfiniteJumpToggle = PlayerTab:CreateToggle({
   Name = "Infinite Jump",
   CurrentValue = false,
   Flag = "InfiniteJump",
   Callback = function(Value)
      getgenv().InfiniteJump = Value
      
      if Value then
         game:GetService("UserInputService").JumpRequest:connect(function()
            if getgenv().InfiniteJump then
               game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):ChangeState("Jumping")
            end
         end)
      end
   end,
})

local NoClipToggle = PlayerTab:CreateToggle({
   Name = "No Clip",
   CurrentValue = false,
   Flag = "NoClip",
   Callback = function(Value)
      getgenv().NoClip = Value
      
      game:GetService("RunService").Stepped:connect(function()
         if getgenv().NoClip then
            for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
               if v:IsA("BasePart") then
                  v.CanCollide = false
               end
            end
         end
      end)
   end,
})

local FlyToggle = PlayerTab:CreateToggle({
   Name = "Fly",
   CurrentValue = false,
   Flag = "Fly",
   Callback = function(Value)
      print("Fly: " .. tostring(Value))
      -- Add your fly script here
   end,
})

-- Misc Tab
local MiscSection = MiscTab:CreateSection("Miscellaneous")

local ConfigToggle = MiscTab:CreateToggle({
   Name = "Auto Save Config",
   CurrentValue = true,
   Flag = "AutoSaveConfig",
   Callback = function(Value)
      print("Auto Save: " .. tostring(Value))
   end,
})

local NotificationsToggle = MiscTab:CreateToggle({
   Name = "Show Notifications",
   CurrentValue = true,
   Flag = "ShowNotifications",
   Callback = function(Value)
      print("Notifications: " .. tostring(Value))
   end,
})

local ThemeDropdown = MiscTab:CreateDropdown({
   Name = "UI Theme",
   Options = {"Default", "Amethyst", "Light"},
   CurrentOption = {"Default"},
   MultipleOptions = false,
   Flag = "Theme",
   Callback = function(Option)
      print("Theme changed to: " .. Option)
   end,
})

local MiscParagraph = MiscTab:CreateParagraph({
   Title = "Rayfield Hub Info",
   Content = "This is a universal script hub using Rayfield Interface Suite. Add your own scripts by creating buttons with loadstring links!"
})

local DestroyButton = MiscTab:CreateButton({
   Name = "Destroy Interface",
   Callback = function()
      Rayfield:Destroy()
   end,
})

print("Rayfield Script Hub loaded successfully!")
